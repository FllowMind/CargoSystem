<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itran.cargosystem.dao.mapper.EmployeeMapper">
	<select id="selectByDateAndPosition" resultType="com.itran.cargosystem.bean.Employee">
		select * from employee 
		<where>
			date = #{date} and
			position = #{position}
		</where>
	</select>
</mapper>